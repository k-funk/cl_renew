{
  "manifest_version": 2,
  "name": "CL Renew",
  "version": "1.0.0",
  "icons": ["icon.png"],
  "content_scripts": [
    {
      "matches": ["https://accounts.craigslist.org/*"],
      "run_at": "document_end",
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
