{
  "manifest_version": 2,
  "name": "CL Renew",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://accounts.craigslist.org/*"],
      "run_at": "document_end",
      "js": ["jquery-2.1.4.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
